<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register_giver</servlet-name>
        <servlet-class>com.sanika.Register_giver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_Taker</servlet-name>
        <servlet-class>com.sanika.Login_Taker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_Giver1</servlet-name>
        <servlet-class>com.sanika.Login_Giver1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout_taker</servlet-name>
        <servlet-class>com.sanika.Logout_taker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registertaker</servlet-name>
        <servlet-class>com.sanika.Registertaker</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register_giver</servlet-name>
        <url-pattern>/Register_giver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_Taker</servlet-name>
        <url-pattern>/Login_Taker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_Giver1</servlet-name>
        <url-pattern>/Login_Giver1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout_taker</servlet-name>
        <url-pattern>/Logout_taker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registertaker</servlet-name>
        <url-pattern>/Registertaker</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
